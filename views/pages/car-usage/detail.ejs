<%- include ../../partials/header.ejs %> <%- include ../../partials/navbar.ejs
%> <%- include ../../partials/sidebar.ejs %>
<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1>Detail</h1>
        </div>
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="/dashboard">Home</a></li>
            <li class="breadcrumb-item">
              <a href="/cars-usage">Penggunaan Mobil</a>
            </li>
            <li class="breadcrumb-item active">Detail Penggunaan Mobil</li>
          </ol>
        </div>
      </div>
    </div>
    <!-- /.container-fluid -->
  </section>

  <!-- Main content -->
  <section class="content">
    <!-- Default box -->
    <div class="card">
      <div class="card-header">
        <h3 class="card-title">Detail Penggunaan Mobil</h3>

        <div class="card-tools">
          <button
            type="button"
            class="btn btn-tool"
            data-card-widget="collapse"
            title="Collapse"
          >
            <i class="fas fa-minus"></i>
          </button>
          <a href="/cars-usage">
            <button type="button" class="btn btn-tool" title="Remove">
              <i class="fas fa-times"></i>
            </button>
          </a>
        </div>
      </div>
      <div class="card-body">
        <div class="row">
          <div class="col-12 col-md-12 col-lg-12 order-2 order-md-1">
            <div class="row">
              <div class="col-12">
                <h5>Driver</h5>
                <div class="post">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user1-128x128.jpg"
                      alt="user image"
                    /> -->
                  </div>
                  <% drivers.forEach(driver => { %> <% if(driver.id ===
                  detailCarUsage.driver_id ) {%>
                  <p><%= driver.name %></p>
                  <% } %> <% }) %>
                </div>
                <h5>Mobil</h5>
                <div class="post clearfix">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user7-128x128.jpg"
                      alt="User Image"
                    /> -->
                  </div>
                  <% cars.forEach(car => { %> <% if(car.id ===
                  detailCarUsage.car_id ) {%>
                  <p><%= car.number_plate %></p>
                  <% } %> <% }) %>
                </div>
                <h5>Tanggal Berangkat</h5>
                <div class="post clearfix">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user7-128x128.jpg"
                      alt="User Image"
                    /> -->
                  </div>
                  <p><%= detailCarUsage.date_departure %></p>
                </div>
                <h5>Tangal Kembali</h5>
                <div class="post">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user1-128x128.jpg"
                      alt="user image"
                    /> -->
                  </div>
                  <!-- /.user-block -->
                  <p><%= detailCarUsage.date_back %></p>
                </div>
                <h5>Lokasi yang Dituju</h5>
                <div class="post">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user1-128x128.jpg"
                      alt="user image"
                    /> -->
                  </div>
                  <!-- /.user-block -->
                  <p><%= detailCarUsage.location_destination %></p>
                </div>
                <h5>Penumpang</h5>
                <div class="post">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user1-128x128.jpg"
                      alt="user image"
                    /> -->
                  </div>
                  <!-- /.user-block -->
                  <p><%= detailCarUsage.passenger %></p>
                </div>
                <% if(role === "superadmin") {%>
                <h5>Organisasi</h5>
                <div class="post">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user1-128x128.jpg"
                      alt="user image"
                    /> -->
                  </div>
                  <!-- /.user-block -->
                  <% if (userOrganizationParsed.length < 2) { %> <%
                  organizations.forEach(organisasi => { %> <% if
                  (organisasi.id.toString() ===
                  userOrganizationParsed[0].toString()) { %>
                  <p><%= organisasi.name %></p>
                  <% } %> <% }) %> <% } else { %>
                  <ul>
                    <% userOrganizationParsed.forEach(orgParseId => { %> <%
                    organizations.forEach(organisasi => { %> <% if
                    (organisasi.id.toString() === orgParseId.toString()) { %>
                    <li><%= organisasi.name %></li>
                    <% } %> <% }) %> <% }) %>
                  </ul>
                  <% } %>
                </div>
                <% } %>
                <h5>KM Terakhir</h5>
                <div class="post">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user1-128x128.jpg"
                      alt="user image"
                    /> -->
                  </div>
                  <!-- /.user-block -->
                  <p><%= detailCarUsage.km_last %></p>
                </div>
                <h5>KM Trip</h5>
                <div class="post">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user1-128x128.jpg"
                      alt="user image"
                    /> -->
                  </div>
                  <!-- /.user-block -->
                  <p><%= detailCarUsage.km_last %></p>
                </div>
                <% if(role === "superadmin") {%>
                <h5>Tarif Tol</h5>
                <div class="post">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user1-128x128.jpg"
                      alt="user image"
                    /> -->
                  </div>
                  <!-- /.user-block -->
                  <% if(detailCarUsage.tol_rates === null) {%>
                  <p>Kosong</p>
                  <% } else { %>
                  <p><%= detailCarUsage.tol_rates %></p>
                  <% } %>
                </div>
                <h5>Tarif Parkir</h5>
                <div class="post">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user1-128x128.jpg"
                      alt="user image"
                    /> -->
                  </div>
                  <!-- /.user-block -->
                  <% if(detailCarUsage.parking_rates === null) {%>
                  <p>Kosong</p>
                  <% } else { %>
                  <p><%= detailCarUsage.parking_rates %></p>
                  <% } %>
                </div>
                <h5>Tarif Bensin</h5>
                <div class="post">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user1-128x128.jpg"
                      alt="user image"
                    /> -->
                  </div>
                  <!-- /.user-block -->
                  <% if(detailCarUsage.fuel_rates === null) {%>
                  <p>Kosong</p>
                  <% } else { %>
                  <p><%= detailCarUsage.fuel_rates %></p>
                  <% } %>
                </div>
                <% } %>
                <h5>Keterangan</h5>
                <div class="post">
                  <div class="user-block">
                    <!-- <img
                      class="img-circle img-bordered-sm"
                      src="../../dist/img/user1-128x128.jpg"
                      alt="user image"
                    /> -->
                  </div>
                  <!-- /.user-block -->
                  <% if(detailCarUsage.note === null) {%>
                  <p>Kosong</p>
                  <% } else { %>
                  <p><%= detailCarUsage.note %></p>
                  <% } %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- /.card-body -->
    </div>
    <!-- /.card -->
  </section>
  <!-- /.content -->
</div>
<%- include ../../partials/footer.ejs %> <%- include ../../partials/js.ejs %>
